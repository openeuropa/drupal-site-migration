{
    "name": "openeuropa/drupal-site-migration",
    "type": "project",
    "license": "EUPL-1.2",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^5.6",
        "league/fractal": "^0.17.0",
        "symfony/config": "^3.4",
        "symfony/console": "^3.4",
        "symfony/dependency-injection": "^3.4",
        "symfony/http-kernel": "^3.4",
        "symfony/yaml": "^3.4"
    },
    "require-dev": {
        "cweagans/composer-patches": "~1.0",
        "openeuropa/code-review": "~1.0@beta"
    },
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "5.6"
        }
    },
    "extra": {
        "enable-patching": true,
        "composer-exit-on-patch-failure": true
    },
    "autoload": {
        "psr-4": {
            "OpenEuropa\\DrupalSiteMigration\\": "./src/"
        },
        "files": [
            "app/AppKernel.php"
        ]
    }
}
